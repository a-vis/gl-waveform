{
  "name": "gl-waveform",
  "version": "4.1.4",
  "description": "Performant waveform renderer with WebGL",
  "main": "dist/gl-waveform.js",
  "umd:main": "dist/gl-waveform.umd.js",
  "module": "dist/gl-waveform.mjs",
  "source": "index.js",
  "scripts": {
    "test": "node test",
    "lint": "eslint */*.js *.js --ignore-pattern test*",
    "demo": "budo demo -- --transform-key=test",
    "test-browser": "budo test -- -t glslify -p tsify",
    "prepublishOnly": "npm run build",
    "build": "rollup -c rollup.config.js",
    "build-multi": "browserify example/multi.js | indexhtmlify | metadataify > example/multi.html",
    "build-data": "browserify example/data.js | indexhtmlify | metadataify > example/data.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-vis/gl-waveform.git"
  },
  "keywords": [
    "waveform",
    "time-data",
    "audio",
    "spectrum",
    "spectrogram",
    "data-vis"
  ],
  "author": "Dmitry Yv <dfcreative@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-vis/gl-waveform/issues"
  },
  "homepage": "https://github.com/a-vis/gl-waveform#readme",
  "dependencies": {
    "color-normalize": "^1.3.0",
    "flatten-vertex-data": "^1.0.2",
    "gl-util": "^3.1.0",
    "is-plain-obj": "^1.1.0",
    "lerp": "^1.0.3",
    "mirrorprop": "^1.0.1",
    "negative-index": "^1.0.3",
    "object-assign": "^4.1.1",
    "offset": "0.0.1",
    "on-idle": "^3.1.4",
    "parse-rect": "^1.2.0",
    "parse-unit": "^1.0.1",
    "pick-by-alias": "^1.2.0",
    "regl": "^1.3.11",
    "to-float32": "^1.0.0",
    "to-px": "^1.1.0",
    "typedarray-pool": "^1.1.0",
    "weak-map": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@zeit/ncc": "^0.1.16",
    "almost-equal": "^1.1.0",
    "audio-oscillator": "^3.1.1",
    "browserify-single-file": "^0.3.0",
    "controlkit": "^0.1.9",
    "fps-indicator": "^1.3.0",
    "gl": "^4.1.1",
    "glslify": "^6.4.1",
    "image-equal": "^4.3.1",
    "image-output": "^2.4.0",
    "image-pixels": "^2.2.0",
    "indexhtmlify": "^1.3.1",
    "is-travis": "^2.0.0",
    "metadataify": "^2.0.0",
    "microbundle": "^0.8.3",
    "pan-zoom": "^3.4.0",
    "performance-now": "^2.1.0",
    "periodic-function": "^1.1.4",
    "prettysize": "^1.1.0",
    "pxls": "^2.3.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-browserify-transform": "^1.0.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "seed-random": "^2.2.0",
    "tsify": "^4.0.1",
    "typescript": "^3.2.2"
  }
}
